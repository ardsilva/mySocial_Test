{
	"installCommand": "yarn install --cwd pwa-react && yarn install --cwd api-react",
	"buildCommand": "cd pwa-react && yarn build",
	"builds": [
		{
			"src": "pwa-react/package.json",
			"use": "@vercel/static-build",
			"config": { "outputDirectory": "build" }
		},
		{
			"src": "api-react/index.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "api-react/index.js"
		},
		{
			"src": "/(.*)",
			"dest": "pwa-react/build/$1"
		}
	]
}
